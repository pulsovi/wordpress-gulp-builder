{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "exactOptionalPropertyTypes": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "./dist",
    "strictNullChecks": true
  },
  "include": ["gulpfile.ts", "builder/**/*.ts"]
}
