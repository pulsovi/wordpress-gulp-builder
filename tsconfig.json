{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "exactOptionalPropertyTypes": true,
    "module": "ES2022",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "dist",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "target": "ES2022"
  },
  "include": ["gulpfile.ts", "global.d.ts", "builder/**/*.ts"]
}
